    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "del": "^6.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
