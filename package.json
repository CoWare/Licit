    "path": "^0.12.7",
    "util": "^0.10.3"
  },
  "devDependencies": {
    "babel-eslint": "^6.1.2",
    "coveralls": "^2.11.13",
    "eslint": "^3.5.0",
    "eslint-config-airbnb": "^11.1.0",
    "eslint-plugin-react": "^6.3.0",
    "expect.js": "^0.3.1",
    "gulp": "^3.9.1",
    "gulp-eslint": "^3.0.1",
    "istanbul": "^0.4.5",
